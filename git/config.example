[user]
  name = Jakub Arnold
  email = darthdeus@gmail.com
[github]
  user = darthdeus
  token =
  oauth-token =
[alias]
  st = status
  ci = commit
  co = checkout
  br = branch
  df = diff
  dc = diff --cached
  amend = commit --amend
  head = !git -l -1
  h = !git head
  r = !git l -20
  ra = !git r --all
  ff = merge --ff-only
  pullff = pull --ff-only
  noff = merge --no-ff
  l = log --graph --abbrev-commit --date=relative
  la = !git l --all
  lg = log -p
  b = branch
  dh1 = diff HEAD~1
  lol = log --graph --decorate --pretty=oneline --abbrev-commit
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
  ls = ls-files
  aa = add --all
  type = cat-file -t
  dump = cat-file -p
[apply]
  whitespace = nowarn
[core]
	excludesfile = ~/.gitignore
  autocrlf = false
	quotepath = false
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow
  frag = magenta
  old = red
  new = green
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[merge]
  tool = vimdiff
  defaultToUpstream = true
[branch]
  autosetupmerge = true
[http]
  sslverify = false
[mergetool]
	keepBackup = true
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path =
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[difftool "Kaleidoscope"]
	cmd = ksdiff-wrapper git \"$LOCAL\" \"$REMOTE\"
[difftool]
	prompt = false
[diff]
	tool = Kaleidoscope
[push]
	default = simple
