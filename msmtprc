defaults
auth           on
tls            on
# tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile        ~/.msmtp.log

account      gmail
host         smtp.gmail.com
port         587
tls on
tls_certcheck off
auth on
from         darthdeus@gmail.com
user         darthdeus
passwordeval "security find-generic-password -s mbsync-gmail-password -w"

account icloud
host smtp.mail.me.com
tls on
tls_certcheck off
auth on
from jakub.arnold@me.com
user jakub.arnold@me.com
passwordeval "security find-generic-password -s mbsync-icloud-password -w"
port 587

account default : gmail
